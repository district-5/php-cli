{
    "name": "district5/cli",
    "description": "Easy to use CLI driven PHP applications.",
    "keywords": ["ohconsole"],
    "homepage": "https://github.com/district-5/php-cli",
    "license": "mit",
    "authors": [
        {
            "name": "Roger Thomas",
            "email": "roger.thomas@district5.co.uk"
        }
    ],
    "require": {
        "php": ">=8.1"
    },
    "autoload": {
        "psr-4": { "District5\\Cli\\": "src/Cli/" }
    },
    "require-dev": {
        "phpunit/phpunit": "*"
    },
    "autoload-dev": {
        "psr-4": {
            "District5\\CliTests\\": "tests/CliTests/",
            "FooBar\\": "tests/CliNamespacePrefixed/"
        },
        "psr-0": {
            "CliExamples\\": "tests/"
        }
    }
}
